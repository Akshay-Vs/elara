{
	"name": "@elara/android",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"command": "chmod +x gradlew && ./gradlew assembleDebug",
				"cwd": "{projectRoot}"
			}
		},
		"release": {
			"executor": "nx:run-commands",
			"options": {
				"command": "chmod +x gradlew && ./gradlew assembleRelease --stacktrace",
				"cwd": "{projectRoot}"
			}
		}
	}
}
